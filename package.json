    "eslint-plugin-format": "^1.0.1",
    "nuxt": "^3.15.4",
    "nuxt-echarts": "^0.2.6",
    "sass": "^1.82.0",
    "typescript": "^5.6.3",
    "vue-tsc": "^2.1.10"
  },
