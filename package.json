    "@iconify/json": "^2.2.310",
    "@nuxtjs/color-mode": "^3.5.2",
    "@pinia/nuxt": "^0.9.0",
    "@primevue/nuxt-module": "^4.3.1",
    "@unocss/eslint-plugin": "^65.5.0",
    "@unocss/nuxt": "^0.65.1",
    "@vueuse/nuxt": "^12.0.0",
    "consola": "^3.2.3",
