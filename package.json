    "dayjs": "^1.11.13",
    "dayjs-nuxt": "^2.1.11",
    "echarts": "^5.6.0",
    "element-plus": "^2.9.0",
    "ioredis": "5.4.2",
    "nuxt-primevue": "^3.0.0",
    "primevue": "^4.3.1",
    "vue-echarts": "^7.0.3",
    "vue-types": "^6.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
